<?xml version="1.0" encoding="UTF-8"?>
<module rename-to="torrentdownload">
    <inherits name="at.torgeir.gwt.common.Common"/>

	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User'/>   	
	
	<!--  inherit css based theme -->
	<!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/>-->
	
	<inherits name='com.smartgwt.SmartGwt' />
	<inherits name="com.smartgwt.tools.SmartGwtTools"/>
	
	<!-- Specify the application specific style sheet. -->
	<stylesheet src='TorrentDownload.css' />
	  
	<!-- Specify the app entry point class. -->
	<entry-point class='at.torgeir.gwt.torrentdownload.client.TorrentDownload'/>
	
	<!-- Specify the paths for translatable code -->
	<source path='client'/>
	<source path='shared'/>
	<source path='test'/>
   
</module>
